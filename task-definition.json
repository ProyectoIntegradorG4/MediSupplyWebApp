{
  "family": "medisupply-task-definition",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::403245568912:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::403245568912:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "medisupply-webapp",
      "image": "403245568912.dkr.ecr.us-east-1.amazonaws.com/medisupply-webapp:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/medisupply-webapp",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ]
    }
  ]
}
